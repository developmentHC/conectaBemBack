{
  "name": "apinode",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "lint": "eslint . --ext .mjs",
    "lint:fix": "eslint . --ext .mjs --fix",
    "test": "vitest --run --reporter verbose --globals",
    "test:watch": "vitest",
    "test:jest": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "build": "npm run lint && node ./src/swagger.mjs && node ./src/index.mjs",
    "dev": "cross-env NODE_ENV=development node ./src/swagger.mjs && nodemon ./src/index.mjs",
    "dev:prod": "cross-env NODE_ENV=production VERCEL=1 VERCEL_URL=conecta-bem-back.vercel.app node ./src/swagger.mjs && nodemon ./src/index.mjs",
    "start": "cross-env NODE_ENV=production node ./src/index.mjs",
    "swagger": "cross-env NODE_ENV=development node ./src/swagger.mjs",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "mongoose": "^8.8.0",
    "multer": "^2.0.2",
    "resend": "^6.5.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@eslint/js": "^9.27.0",
    "@jest/globals": "^30.0.5",
    "cross-env": "^10.0.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.1.7",
    "prettier": "3.5.3",
    "supertest": "^7.0.0",
    "vitest": "^2.1.8"
  }
}
